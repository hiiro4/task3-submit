
<div class="container">
    <div class="row">
<%= render"books/list" %>
<div class="col-md-8 offset-md-1">

<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  <%@users.each do |users|%>
  <tbody>
     <tr>
          <td><%=image_tag users.get_profile_image(100,100) %></td>
          <td><%=users.name %></td>
         <td><%=link_to"Show",user_path(users.id)%></td>
     </tr>
      <%end%>
   </tbody>
  </table>
  </div>
</div>
 </div>
